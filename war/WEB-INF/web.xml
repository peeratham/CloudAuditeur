	<servlet>
		<servlet-name>DownloadModel</servlet-name>
		<servlet-class>uva.cs.auditeur.cloud.DownloadModelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DownloadModel</servlet-name>
		<url-pattern>/download</url-pattern>
	</servlet-mapping>	
	
	<welcome-file-list>
		<welcome-file>service.jsp</welcome-file>
	</welcome-file-list>
</web-app>
